{
  "name": "contributte/redis",
  "description": "Redis client integration into Nette framework",
  "keywords": [
    "nette",
    "redis",
    "cache",
    "predis"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/redis",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": ">=8.2",
    "nette/di": "^3.2.4",
    "predis/predis": "^2.3.0"
  },
  "suggest": {
    "ext-igbinary": "For Igbinary serialization",
    "ext-snappy": "For Snappy serialization"
  },
  "require-dev": {
    "nette/caching": "^3.1.3",
    "nette/http": "^3.0.1",
    "contributte/qa": "^0.4.0",
    "contributte/phpstan": "^0.3.0",
    "contributte/tester": "^0.4.0",
    "mockery/mockery": "^1.5.1",
    "tracy/tracy": "^2.10.9"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Redis\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
